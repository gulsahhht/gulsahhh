#include <stdio.h>
#include <stdlib.h>


struct noktalar {

	int x,y;
};
struct noktalar a,b;

int main() {
	int i,t,n;
	float uzaklik;

	i=1;
    n=1;
	 for(i=1;i<6;i+=1)
	 
	    { 
		
	   	for(n=1;n!=11;n+=1)
		   t=5;   
		{ while(i!=t){
		
	 	printf("%d. noktanin koordinatlarini giriniz.",i);
	    scanf("%d%d",&a.x,&a.y);
	    printf("%d. noktanin koordinatlarini giriniz.",t);
	    scanf("%d%d",&b.x,&b.y);
	    uzaklik=sqrt(pow((a.x-b.x),2)+pow((a.y-b.y),2));
	   printf("%d. ve %d. nokta arasindaki uzaklik: %.2f \n",i,t,uzaklik);
	   t-=1;
       } 
    }
        
	  }
	    
	 
	return 0;
}
